<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <androidx.appcompat.widget.AppCompatImageView
        android:id="@+id/backKeyboardElementIV"
        android:layout_width="0dp"
        android:layout_height="0dp"
		android:alpha="0.3"
		android:scaleType="centerInside"
		android:scaleX="0.5"
		android:scaleY="0.5"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toEndOf="@+id/zeroKeyboardElementB"
        app:layout_constraintTop_toBottomOf="@+id/ninthKeyboardElementTV"
		app:srcCompat="@drawable/delete_1" />

    <androidx.appcompat.widget.AppCompatTextView
        android:id="@+id/zeroKeyboardElementB"
        style="@style/roboto_medium__32__black"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:gravity="center"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toStartOf="@+id/backKeyboardElementIV"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toEndOf="@+id/tenthKeyboardElementTV"
        app:layout_constraintTop_toBottomOf="@+id/eighthKeyboardElementTV"
        tools:text="0" />

    <androidx.appcompat.widget.AppCompatTextView
        android:id="@+id/fourthKeyboardElementTV"
        style="@style/roboto_medium__32__black"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:gravity="center"
        app:layout_constraintBottom_toTopOf="@+id/seventhKeyboardElementTV"
        app:layout_constraintEnd_toStartOf="@+id/fifthKeyboardElementTV"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/firstKeyboardElementTV"
        tools:text="4" />

    <androidx.appcompat.widget.AppCompatTextView
        android:id="@+id/firstKeyboardElementTV"
        style="@style/roboto_medium__32__black"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:gravity="center"
        app:layout_constraintBottom_toTopOf="@+id/fourthKeyboardElementTV"
        app:layout_constraintEnd_toStartOf="@+id/secondKeyboardElementTV"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        tools:text="1" />

    <androidx.appcompat.widget.AppCompatTextView
        android:id="@+id/thirdKeyboardElementTV"
        style="@style/roboto_medium__32__black"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:gravity="center"
        app:layout_constraintBottom_toTopOf="@+id/sixthKeyboardElementTV"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toEndOf="@+id/secondKeyboardElementTV"
        app:layout_constraintTop_toTopOf="parent"
        tools:text="3" />

    <androidx.appcompat.widget.AppCompatTextView
        android:id="@+id/secondKeyboardElementTV"
        style="@style/roboto_medium__32__black"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:gravity="center"
        app:layout_constraintBottom_toTopOf="@+id/fifthKeyboardElementTV"
        app:layout_constraintEnd_toStartOf="@+id/thirdKeyboardElementTV"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toEndOf="@+id/firstKeyboardElementTV"
        app:layout_constraintTop_toTopOf="parent"
        tools:text="2" />

    <androidx.appcompat.widget.AppCompatTextView
        android:id="@+id/fifthKeyboardElementTV"
        style="@style/roboto_medium__32__black"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:gravity="center"
        app:layout_constraintBottom_toTopOf="@+id/eighthKeyboardElementTV"
        app:layout_constraintEnd_toStartOf="@+id/sixthKeyboardElementTV"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toEndOf="@+id/fourthKeyboardElementTV"
        app:layout_constraintTop_toBottomOf="@+id/secondKeyboardElementTV"
        tools:text="5" />

    <androidx.appcompat.widget.AppCompatTextView
        android:id="@+id/sixthKeyboardElementTV"
        style="@style/roboto_medium__32__black"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:gravity="center"
        app:layout_constraintBottom_toTopOf="@+id/ninthKeyboardElementTV"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toEndOf="@+id/fifthKeyboardElementTV"
        app:layout_constraintTop_toBottomOf="@+id/thirdKeyboardElementTV"
        tools:text="6" />

    <androidx.appcompat.widget.AppCompatTextView
        android:id="@+id/seventhKeyboardElementTV"
        style="@style/roboto_medium__32__black"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:gravity="center"
        app:layout_constraintBottom_toTopOf="@+id/tenthKeyboardElementTV"
        app:layout_constraintEnd_toStartOf="@+id/eighthKeyboardElementTV"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/fourthKeyboardElementTV"
        tools:text="7" />

    <androidx.appcompat.widget.AppCompatTextView
        android:id="@+id/eighthKeyboardElementTV"
        style="@style/roboto_medium__32__black"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:gravity="center"
        app:layout_constraintBottom_toTopOf="@+id/zeroKeyboardElementB"
        app:layout_constraintEnd_toStartOf="@+id/ninthKeyboardElementTV"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toEndOf="@+id/seventhKeyboardElementTV"
        app:layout_constraintTop_toBottomOf="@+id/fifthKeyboardElementTV"
        tools:text="8" />

    <androidx.appcompat.widget.AppCompatTextView
        android:id="@+id/ninthKeyboardElementTV"
        style="@style/roboto_medium__32__black"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:gravity="center"
        app:layout_constraintBottom_toTopOf="@+id/backKeyboardElementIV"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toEndOf="@+id/eighthKeyboardElementTV"
        app:layout_constraintTop_toBottomOf="@+id/sixthKeyboardElementTV"
        tools:text="9" />

    <androidx.appcompat.widget.AppCompatTextView
        android:id="@+id/tenthKeyboardElementTV"
        style="@style/roboto_medium__32__black"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:gravity="center"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toStartOf="@+id/zeroKeyboardElementB"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/seventhKeyboardElementTV" />

</androidx.constraintlayout.widget.ConstraintLayout>